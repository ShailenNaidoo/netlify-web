#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/shailen/Desktop/Personal/netlify-web/netlify-web/node_modules/.pnpm/rc@1.2.8/node_modules/rc/cli.js/node_modules:/home/shailen/Desktop/Personal/netlify-web/netlify-web/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/home/shailen/Desktop/Personal/netlify-web/netlify-web/node_modules/.pnpm/rc@1.2.8/node_modules:/home/shailen/Desktop/Personal/netlify-web/netlify-web/node_modules/.pnpm/node_modules:/home/shailen/Desktop/Personal/netlify-web/netlify-web/node_modules:/home/shailen/Desktop/Personal/netlify-web/node_modules:/home/shailen/Desktop/Personal/node_modules:/home/shailen/Desktop/node_modules:/home/shailen/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
